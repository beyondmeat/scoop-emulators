        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.13.2-2497-gbef5d2b51&platform=windows-amd64#/dl.zip",
            "hash": "26b1c9782733385ef2965feb08f0930cf173d779aade7671b0094dfab2c3ae37",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
