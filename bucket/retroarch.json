{
    "homepage": "https://www.retroarch.com/",
    "description": "Frontend for emulators, game engines and media players.",
    "license": "GPL-3.0-only",
    "version": "1.9.2",
    "architecture": {
        "64bit": {
            "url": "https://buildbot.libretro.com/stable/1.9.2/windows/x86_64/RetroArch.7z",
            "hash": "43295d3c71580a4bac1370cd19e5927a63577850348dab29d258b0ca36cbbfd2"
        },
        "32bit": {
            "url": "https://buildbot.libretro.com/stable/1.9.2/windows/x86/RetroArch.7z",
            "hash": "6866a692e56ac86498b97211d067f3c85ec94b7ca863cf7e7bbcd86824c5eabc"
        }
    },
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\retroarch.cfg\")) {",
        "    New-Item \"$dir\\retroarch.cfg\" -Type File | Out-Null",
        "}"
    ],
    "bin": [
        [
            "retroarch.exe",
            "retroarch"
        ]
    ],
    "shortcuts": [
        [
            "retroarch.exe",
            "RetroArch"
        ]
    ],
    "persist": [
        "assets\\wallpapers",
        "cores",
        "cheats",
        "overlays",
        "playlists",
        "records",
        "recordings",
        "saves",
        "screenshots",
        "states",
        "system",
        "thumbnails",
        "retroarch.cfg"
    ],
    "checkver": {
        "url": "https://www.retroarch.com/?page=platforms",
        "regex": "The current stable version is: (?<main>[\\d.]+)(\\s*(\\(|\\[)(\\w+)(\\)|\\]))?",
        "replace": "${5}${3}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://buildbot.libretro.com/stable/$matchMain/windows/x86_64/RetroArch.7z"
            },
            "32bit": {
                "url": "https://buildbot.libretro.com/stable/$matchMain/windows/x86/RetroArch.7z"
            }
        }
    }
}
