{
    "homepage": "https://www.retroarch.com/",
    "description": "Frontend for emulators, game engines and media players.",
    "license": "GPL-3.0-only",
    "version": "1.9.5",
    "architecture": {
        "64bit": {
            "url": "https://buildbot.libretro.com/stable/1.9.5/windows/x86_64/RetroArch.7z",
            "hash": "d1566824d4d534713755e7cf6267e921efb21bf37a4134629f56ad614bd15464"
        },
        "32bit": {
            "url": "https://buildbot.libretro.com/stable/1.9.5/windows/x86/RetroArch.7z",
            "hash": "8507ebcb1c404625d892c3dc16a272bc06c66e61a480a589a1fab45dd573d964"
        }
    },
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\retroarch.cfg\")) {",
        "    New-Item \"$dir\\retroarch.cfg\" -Type File | Out-Null",
        "}"
    ],
    "bin": [
        [
            "retroarch.exe",
            "retroarch"
        ]
    ],
    "shortcuts": [
        [
            "retroarch.exe",
            "RetroArch"
        ]
    ],
    "persist": [
        "assets\\wallpapers",
        "cores",
        "cheats",
        "overlays",
        "playlists",
        "records",
        "recordings",
        "saves",
        "screenshots",
        "states",
        "system",
        "thumbnails",
        "retroarch.cfg"
    ],
    "checkver": {
        "url": "https://www.retroarch.com/?page=platforms",
        "regex": "The current stable version is: (?<main>[\\d.]+)(\\s*(\\(|\\[)(\\w+)(\\)|\\]))?",
        "replace": "${5}${3}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://buildbot.libretro.com/stable/$matchMain/windows/x86_64/RetroArch.7z"
            },
            "32bit": {
                "url": "https://buildbot.libretro.com/stable/$matchMain/windows/x86/RetroArch.7z"
            }
        }
    }
}
