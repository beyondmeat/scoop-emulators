{
    "homepage": "https://www.retroarch.com/",
    "description": "Frontend for emulators, game engines and media players.",
    "license": "GPL-3.0-only",
    "version": "1.9.3",
    "architecture": {
        "64bit": {
            "url": "https://buildbot.libretro.com/stable/1.9.3/windows/x86_64/RetroArch.7z",
            "hash": "addb1c3821b15492ea86e577ae5a1a167818960b91b5f0a89615c038651be25b"
        },
        "32bit": {
            "url": "https://buildbot.libretro.com/stable/1.9.3/windows/x86/RetroArch.7z",
            "hash": "47f0436349f1abee8cee2555e619a687b522c8a66a824f8079eed946ca720b13"
        }
    },
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\retroarch.cfg\")) {",
        "    New-Item \"$dir\\retroarch.cfg\" -Type File | Out-Null",
        "}"
    ],
    "bin": [
        [
            "retroarch.exe",
            "retroarch"
        ]
    ],
    "shortcuts": [
        [
            "retroarch.exe",
            "RetroArch"
        ]
    ],
    "persist": [
        "assets\\wallpapers",
        "cores",
        "cheats",
        "overlays",
        "playlists",
        "records",
        "recordings",
        "saves",
        "screenshots",
        "states",
        "system",
        "thumbnails",
        "retroarch.cfg"
    ],
    "checkver": {
        "url": "https://www.retroarch.com/?page=platforms",
        "regex": "The current stable version is: (?<main>[\\d.]+)(\\s*(\\(|\\[)(\\w+)(\\)|\\]))?",
        "replace": "${5}${3}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://buildbot.libretro.com/stable/$matchMain/windows/x86_64/RetroArch.7z"
            },
            "32bit": {
                "url": "https://buildbot.libretro.com/stable/$matchMain/windows/x86/RetroArch.7z"
            }
        }
    }
}
