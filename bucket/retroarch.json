{
    "homepage": "https://www.retroarch.com/",
    "description": "Frontend for emulators, game engines and media players.",
    "license": "GPL-3.0-only",
    "version": "1.9.4",
    "architecture": {
        "64bit": {
            "url": "https://buildbot.libretro.com/stable/1.9.4/windows/x86_64/RetroArch.7z",
            "hash": "549372470f8ec7941cf2cbca2e8b1921a9d5f05c25f13775d03ccd1e206c1fc1"
        },
        "32bit": {
            "url": "https://buildbot.libretro.com/stable/1.9.4/windows/x86/RetroArch.7z",
            "hash": "526131c667d56b52ab07f209c02f79094c02cd5cbd09dea73926c554b3ca8d21"
        }
    },
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\retroarch.cfg\")) {",
        "    New-Item \"$dir\\retroarch.cfg\" -Type File | Out-Null",
        "}"
    ],
    "bin": [
        [
            "retroarch.exe",
            "retroarch"
        ]
    ],
    "shortcuts": [
        [
            "retroarch.exe",
            "RetroArch"
        ]
    ],
    "persist": [
        "assets\\wallpapers",
        "cores",
        "cheats",
        "overlays",
        "playlists",
        "records",
        "recordings",
        "saves",
        "screenshots",
        "states",
        "system",
        "thumbnails",
        "retroarch.cfg"
    ],
    "checkver": {
        "url": "https://www.retroarch.com/?page=platforms",
        "regex": "The current stable version is: (?<main>[\\d.]+)(\\s*(\\(|\\[)(\\w+)(\\)|\\]))?",
        "replace": "${5}${3}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://buildbot.libretro.com/stable/$matchMain/windows/x86_64/RetroArch.7z"
            },
            "32bit": {
                "url": "https://buildbot.libretro.com/stable/$matchMain/windows/x86/RetroArch.7z"
            }
        }
    }
}
