        ]
    },
    "checkver": {
        "re": "cemuhook_(?<cemu>[\\d_]+).zip\">Cemu hook ([\\d.]+) for"
    },
    "autoupdate": {
        "url": "https://files.sshnuke.net/cemuhook_$matchCemu.zip"
    }
